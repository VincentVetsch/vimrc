class ProjectCenter():
    '''
    Project center is the class that helps create a new project under a svn repository.
    It will help create the required files for the classes within your project with
    unittest, docstring documents, uml diagrams, and implementation files.
    '''

    def __init__(self, x):
        '''
        Object Constructor
        '''
        self.x = x

    def y(self):
        #import pdb
        pdb.set_trace()
        #breakpoint1()
        print "X = " + str(self.x)
        return self.x


if __name__ == '__main__':
    #print "Create the Object"
    #breakpoint1 = Tracer()
    pc = ProjectCenter(29)
    pc.y()
