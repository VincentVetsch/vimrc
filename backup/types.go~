package main
import (
    "math/cmplx"
    "fmt"
)

var (
    ToBe bool = false
    MaxInt uint64 = 1<<64 -1
    z complex128 = cmplx.Sqrt(-5+12i)
)

func main() {
    const f = "%T(%v)\n" // %T = type, %v = value (repr)
    fmt.Printf(f, ToBe, ToBe)
    fmt.Printf(f, MaxInt, MaxInt)
    fmt.Printf(f, z, z)
}
