package main

import "fmt"

type Vertex struct {
    Lat float64
    Long float64
}
var m = map[string]Vertex {
    "Bell Labs": {40.68433, -74.39967},
    "Google": {37.42202, -122.08408},
}
var d = []string{"A", "b", "Hello"} //String array or slice

func main() {
    fmt.Println(m) //Print the map Vertex

    n := make(map[string]int)
    n["Answer"] = 42
    fmt.Println("The value:", n["Answer"])
    n["Answer"] += 6
    fmt.Println("The value:", n["Answer"])
    delete(n, "Answer")
    fmt.Println("The value:", n["Answer"])
    v, ok := n["Answer"]
    fmt.Println("The value:", v, "Present?", ok)
    n["Answer1"] = 50
    v, ok1 := n["Answer1"] // cannot re-assign ok
    fmt.Println("The value:", v, "Present?", ok1)
    fmt.Println(d)
}
