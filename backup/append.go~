package main

import "fmt"

type str struct {
        S       string
        I       int
}



func main() {
        var ss []string

        ss = append(ss, "once upon a time")
        ss = append(ss, "you dressed so fine")
        var myStr []str
        for i, v := range ss {
                st := str{v, i}
                myStr = append(myStr, st)
        }
        for _, v := range myStr {
                fmt.Println(v.S, v.I)
        }
}
