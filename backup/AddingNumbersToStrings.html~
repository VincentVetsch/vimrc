            function myFunction()
                {
                    var x = 5 + 5;
                    var y = "5" + 5;
                    var z = "Hello" + 5;
                    var demoP = document.getElementById("demo");
                    demoP.innerHTML = x + "<br>" + y + "<br>" + z;
                }
        </script>
        <title>Add Numbers to Strings</title>
