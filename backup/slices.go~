package main

import "fmt"

func main() {
    p := []int{2, 3, 5, 7, 11, 13}
    p = append(p, 15, 22, 32)
    fmt.Println("p == ", p)
    for i := 1; i < 100000; i += 4 {
        p = append(p, i)
    }
    for i := 0; i < len(p); i++ {
        fmt.Printf("p{%d} == %d\n", i, p[i])
    }
    fmt.Println(p)
}

